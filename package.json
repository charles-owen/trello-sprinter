{
  "name": "trello-sprinter",
  "version": "2.0.1",
  "description": "Provides summarizations of sprint activity in a Trello project",
  "main": "dist/trello-sprinter.min.js",
  "module": "index.js",
  "private": false,
  "scripts": {
    "build:dev": "npx webpack --config webpack.dev.js",
    "build:prod": "npx webpack --config webpack.prod.js",
    "build:all": "npm run build:dev && npm run build:prod",
    "serve": "webpack-dev-server --config webpack.dev.js --open"
  },
  "keywords": [
    "Trello",
    "Project Monitoring",
    "Student Projects"
  ],
  "author": "Charles B. Owen",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.13",
    "@babel/runtime": "7.12.13",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.1.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "resolve-url-loader": "^3.1.2",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.21.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "4.0.0-beta.0",
    "webpack-merge": "^5.7.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3"
  },
  "dependencies": {
    "trello-connect": "~2.0.4"
  }
}
