{
  "name": "trello-sprinter",
  "version": "2.0.4",
  "description": "Provides summarizations of sprint activity in a Trello project",
  "main": "dist/trello-sprinter.min.js",
  "module": "index.js",
  "scripts": {
    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "build:all": "$npm_execpath run build:dev && $npm_execpath run build:prod",
    "serve": "webpack-dev-server --config webpack.dev.js --open"
  },
  "keywords": [
    "Trello",
    "Project Monitoring",
    "Student Projects"
  ],
  "author": "Charles B. Owen",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "7.17.2",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.6.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "css-what": "^6.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "4.7.4",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "trello-connect": "~2.0.7"
  }
}
